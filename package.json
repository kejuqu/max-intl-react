    "react-intl": "^6.6.8"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.2.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "babel-cli": "^6.26.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.5.0",
